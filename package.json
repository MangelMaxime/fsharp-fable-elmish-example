{
  "name": "fsharp-fable-elmish-example",
  "version": "0.0.1",
  "description": "F# Fable Elmish example",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurentpayot/fsharp-fable-elmish-example.git"
  },
  "keywords": [
    "F#",
    "Fable",
    "Elmish",
    "Preact",
    "Vite",
    "example",
    "Elm",
    "counter",
    "routing",
    "subscription",
    "decoding"
  ],
  "author": "Laurent Payot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laurentpayot/fsharp-fable-elmish-example/issues"
  },
  "homepage": "https://github.com/laurentpayot/fsharp-fable-elmish-example#readme",
  "type": "module",
  "scripts": {
    "restore": "dotnet tool restore && dotnet paket restore",
    "postinstall": "pnpm restore",
    "test": "pnpm test",
    "links": "cd output/src && ln -s ../../src/index.html",
    "start": "dotnet fable watch --outDir output --sourceMaps --run pnpm vite",
    "build": "dotnet fable --outDir output --run pnpm vite build",
    "serve": "pnpm dlx http-server dist -a localhost --port 5000 --proxy http://localhost:5000?",
    "do-update": "pnpm update --latest && dotnet paket update"
  },
  "devDependencies": {
    "terser": "^5.24.0",
    "vite": "^5.0.4"
  },
  "dependencies": {
    "preact": "^10.19.2"
  }
}
