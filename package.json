{
  "name": "fsharp-counter-web-app",
  "version": "0.0.1",
  "description": "F# counter web app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurentpayot/Fsharp-counter-web-app.git"
  },
  "keywords": [
    "F#",
    "counter",
    "Vite",
    "Fable",
    "Elmish"
  ],
  "author": "Laurent Payot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laurentpayot/Fsharp-counter-web-app/issues"
  },
  "homepage": "https://github.com/laurentpayot/Fsharp-counter-web-app#readme",
  "type": "module",
  "scripts": {
    "postinstall": "dotnet tool restore && dotnet restore && dotnet build",
    "test": "pnpm test",
    "start": "dotnet fable watch --outDir output --sourceMaps --run pnpm vite",
    "build": "dotnet fable --outDir output --run pnpm vite build",
    "serve": "cd dist && python3 -m http.server 5000",
    "do-update": "pnpm update --latest && dotnet outdated --upgrade"
  },
  "devDependencies": {
    "terser": "^5.24.0",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "preact": "^10.19.2"
  }
}
